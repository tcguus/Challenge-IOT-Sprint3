  {
    "id": "ac85501af9dde29c",
    "type": "function",
    "z": "f356cfe274aa46de",
    "name": "Criar Tabelas (SQL)",
    "func": "\nmsg.topic = `\nCREATE TABLE IF NOT EXISTS Vagas (\n    deviceId TEXT PRIMARY KEY,\n    status TEXT,\n    maintenance BOOLEAN,\n    last_update INTEGER\n);\n\nCREATE TABLE IF NOT EXISTS Telemetria (\n    id INTEGER PRIMARY KEY AUTOINCREMENT,\n    deviceId TEXT,\n    distance_cm REAL,\n    temp_c REAL,\n    hum_pct REAL,\n    rssi INTEGER,\n    ts INTEGER\n);\n`;\nreturn msg;",
