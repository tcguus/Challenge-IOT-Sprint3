  {
    "id": "b41bca7bd9bd4b3d",
    "type": "ui_template",
    "z": "f356cfe274aa46de",
    "group": "2ed522db7c3185ef",
    "name": "",
    "order": 0,
    "width": 0,
    "height": 0,
    "format": "<style>\n    .parking-grid {\n        display: grid;\n        grid-template-columns: 1fr 1fr 1fr;\n        /* 3 colunas */\n        gap: 15px;\n        /* Espaço entre as vagas */\n        padding: 10px;\n    }\n\n    .vaga-box {\n        border: 2px solid #555;\n        border-radius: 10px;\n        padding: 10px;\n        text-align: center;\n        font-size: 1.2em;\n        font-weight: bold;\n        transition: all 0.3s ease;\n    }\n\n    .vaga-box h3 {\n        margin: 0 0 10px 0;\n        padding-bottom: 5px;\n        border-bottom: 1px solid #666;\n        text-transform: capitalize;\n    }\n\n    .vaga-status {\n        font-size: 1.5em;\n        text-transform: uppercase;\n    }\n\n    /* Classes dinâmicas que o Node-RED vai aplicar */\n    .livre {\n        background-color: #4CAF50;\n        color: white;\n    }\n\n    .ocupada {\n        background-color: #F44336;\n        color: white;\n    }\n\n    .manutencao {\n        background-color: #FFC107;\n        /* Amarelo */\n        color: #333;\n        animation: pulse 1.5s infinite;\n        /* Animação a piscar */\n    }\n\n    .offline {\n        background-color: #9E9E9E;\n        color: #333;\n    }\n\n    /* Animação para a manutenção */\n    @keyframes pulse {\n        0% {\n            box-shadow: 0 0 0 0 rgba(255, 193, 7, 0.7);\n        }\n\n        70% {\n            box-shadow: 0 0 0 10px rgba(255, 193, 7, 0);\n        }\n\n        100% {\n            box-shadow: 0 0 0 0 rgba(255, 193, 7, 0);\n        }\n    }\n</style>\n\n<div class=\"parking-grid\">\n\n    <div class=\"vaga-box\" ng-class=\"{\n            'manutencao': (msg.payload.vagaA || {}).maintenance,\n            'ocupada':    !(msg.payload.vagaA || {}).maintenance && (msg.payload.vagaA || {}).status == 'ocupada',\n            'livre':      !(msg.payload.vagaA || {}).maintenance && (msg.payload.vagaA || {}).status == 'livre',\n            'offline':    !(msg.payload.vagaA || {}).status\n         }\">\n        <h3>Vaga A</h3>\n        <div class=\"vaga-status\">\n            {{(msg.payload.vagaA || {}).maintenance ? 'MANUTENÇÃO' : (msg.payload.vagaA || {}).status || 'OFFLINE'}}\n        </div>\n    </div>\n\n    <div class=\"vaga-box\" ng-class=\"{\n            'manutencao': (msg.payload.vagaB || {}).maintenance,\n            'ocupada':    !(msg.payload.vagaB || {}).maintenance && (msg.payload.vagaB || {}).status == 'ocupada',\n            'livre':      !(msg.payload.vagaB || {}).maintenance && (msg.payload.vagaB || {}).status == 'livre',\n            'offline':    !(msg.payload.vagaB || {}).status\n         }\">\n        <h3>Vaga B</h3>\n        <div class=\"vaga-status\">\n            {{(msg.payload.vagaB || {}).maintenance ? 'MANUTENÇÃO' : (msg.payload.vagaB || {}).status || 'OFFLINE'}}\n        </div>\n    </div>\n\n    <div class=\"vaga-box\" ng-class=\"{\n            'manutencao': (msg.payload.vagaC || {}).maintenance,\n            'ocupada':    !(msg.payload.vagaC || {}).maintenance && (msg.payload.vagaC || {}).status == 'ocupada',\n            'livre':      !(msg.payload.vagaC || {}).maintenance && (msg.payload.vagaC || {}).status == 'livre',\n            'offline':    !(msg.payload.vagaC || {}).status\n         }\">\n        <h3>Vaga C</h3>\n        <div class=\"vaga-status\">\n            {{(msg.payload.vagaC || {}).maintenance ? 'MANUTENÇÃO' : (msg.payload.vagaC || {}).status || 'OFFLINE'}}\n        </div>\n    </div>\n</div>",
